{
  "name": "@typegrahql/server",
  "version": "0.0.1",
  "description": "",
  "main": "index.js",
  "scripts": {
    "test-ci": "yarn run test",
    "server": "yarn start",
    "compile": "tsc",
    "test": "jest --runInBand --forceExit --detectOpenHandles",
    "clean": "rm -rf .compiled .serverless",
    "lint": "eslint --fix 'src/**/*.{tsx,ts,js}'",
    "start": "env-cmd --fallback ./stages/local.env sh -c 'sls offline --skipCacheInvalidation --useSeparateProcesses -o 0.0.0.0'",
    "git-cz": "cd ../../ && npx git-cz"
  },
  "_moduleAliases": {
    "@common": ".compiled/src/common",
    "@config": ".compiled/config"
  },
  "prettier": {
    "printWidth": 120,
    "tabWidth": 2,
    "semi": true,
    "singleQuote": true,
    "trailingComma": "all",
    "bracketSpacing": true
  },
  "keywords": [],
  "author": "Vishesh Duggar",
  "devDependencies": {
    "@babel/cli": "7.4.4",
    "@babel/core": "7.4.5",
    "@babel/preset-env": "7.4.5",
    "@babel/preset-react": "7.0.0",
    "@babel/preset-typescript": "7.3.3",
    "@commitlint/cli": "8.0.0",
    "@commitlint/config-conventional": "8.0.0",
    "@types/aws-lambda": "8.10.11",
    "@types/basic-auth": "1.1.2",
    "@types/bcryptjs": "2.4.1",
    "@types/bson": "1.0.11",
    "@types/graphql": "14.2.2",
    "@types/inquirer": "6.0.3",
    "@types/jest": "24.0.0",
    "@types/jsonwebtoken": "7.2.8",
    "@types/mongodb": "3.1.28",
    "@types/mongodb-memory-server": "1.8.0",
    "@types/mongoose": "5.5.6",
    "@types/mongoose-unique-validator": "1.0.1",
    "@types/node": "8.10.0",
    "@types/nodemailer": "4.6.2",
    "@types/semver": "6.0.0",
    "@types/uuid": "3.4.3",
    "@types/yargs": "13.0.0",
    "@typescript-eslint/eslint-plugin": "1.10.2",
    "@typescript-eslint/parser": "1.10.2",
    "aws-sdk": "2.311.0",
    "babel-core": "7.0.0-bridge.0",
    "commitizen": "3.1.1",
    "cz-conventional-changelog": "2.1.0",
    "env-cmd": "8.0.2",
    "eslint": "5.3.0",
    "eslint-config-airbnb": "17.1.0",
    "eslint-config-prettier": "5.0.0",
    "eslint-plugin-import": "2.17.3",
    "eslint-plugin-jsx-a11y": "6.2.1",
    "eslint-plugin-prettier": "3.1.0",
    "eslint-plugin-react": "7.13.0",
    "fastlint": "^1.1.0",
    "git-tag-version": "1.3.1",
    "husky": "2.4.0",
    "inquirer": "6.3.1",
    "jest": "24.0.0",
    "module-alias": "2.2.0",
    "nodemon": "1.17.5",
    "prettier": "1.18.2",
    "pug": "2.0.3",
    "pug-loader": "2.4.0",
    "rename-cli": "6.0.0",
    "serverless": "1.46.1",
    "serverless-offline": "5.5.0",
    "serverless-webpack": "5.3.1",
    "ts-jest": "24.0.0",
    "ts-loader": "4.4.1",
    "ts-node": "3.3.0",
    "tspath": "1.2.10",
    "typescript": "3.4.5",
    "unroll": "1.5.0",
    "webpack": "4.35.0",
    "webpack-command": "0.4.2",
    "webpack-graphql-loader": "1.0.2",
    "yaml": "1.5.0",
    "yargs": "13.2.4"
  },
  "dependencies": {
    "apollo-server-lambda": "2.6.7",
    "aws-lambda": "0.1.2",
    "basic-auth": "2.0.1",
    "bcryptjs": "2.4.3",
    "bson": "4.0.2",
    "date-fns": "1.29.0",
    "fcm-push": "1.1.3",
    "graphql": "14.4.1",
    "graphql-auth": "0.2.6",
    "graphql-playground-middleware-lambda": "1.7.13",
    "graphql-tools": "4.0.5",
    "jsonwebtoken": "8.5.1",
    "lodash": "4.17.11",
    "migrate-mongoose": "3.2.2",
    "mongodb-memory-server": "5.1.5",
    "mongoose": "5.6.2",
    "mongoose-unique-validator": "2.0.1",
    "nodemailer": "6.4.16",
    "reflect-metadata": "0.1.13",
    "rrule": "2.5.5",
    "semver": "6.0.0",
    "serverless-plugin-existing-s3": "2.4.0",
    "type-graphql": "0.17.4",
    "typegoose": "5.7.2",
    "typedi": "0.8.0",
    "tslib": "1.10.0",
    "validator": "11.0.0",
    "winston": "3.2.1",
    "xlsx": "0.14.3"
  },
  "jest": {
    "globals": {
      "ts-jest": {
        "tsConfigFile": "tsconfig.json"
      }
    },
    "moduleFileExtensions": [
      "ts",
      "js",
      "json"
    ],
    "moduleDirectories": [
      "node_modules",
      "src"
    ],
    "moduleNameMapper": {
      "@common/(.*)": "<rootDir>/src/common/$1",
      "@config/(.*)": "<rootDir>/config/$1",
      "\\.(pug)$": "<rootDir>/src/constants/email-templates/__mocks__/pugMock.js"
    },
    "globalSetup": "<rootDir>/config/tests/setup_hook.js",
    "globalTeardown": "<rootDir>/config/tests/teardown_hook.js",
    "transform": {
      ".+\\.(ts|tsx)$": "./node_modules/ts-jest/preprocessor.js"
    },
    "testRegex": ".(test|spec)\\.(ts)$",
    "testEnvironment": "node",
    "collectCoverage": true,
    "collectCoverageFrom": [
      "**/*.{ts,jsx}",
      "!**/node_modules/**",
      "!**/coverage/**",
      "!**/webpack.config.js",
      "!**/query.ts",
      "!**/resolvers.ts",
      "!**/typings/**",
      "!handler.ts"
    ]
  }
}
